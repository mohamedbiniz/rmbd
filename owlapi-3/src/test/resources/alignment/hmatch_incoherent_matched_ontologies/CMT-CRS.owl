      <owl:Class rdf:about="http://CMT.owl#Document"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Preference"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#ProgramCommittee"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="http://CMT.owl#Conference"/>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Decision"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Person"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="http://CMT.owl#Preference">

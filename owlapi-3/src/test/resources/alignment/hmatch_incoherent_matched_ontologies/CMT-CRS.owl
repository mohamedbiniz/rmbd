      <owl:Class rdf:about="http://CMT.owl#ProgramCommitteeChair"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://CMT.owl#ConferenceChair"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://CMT.owl#Chairman"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://CMT.owl#AssociatedChair"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://CRS.owl#person"/>
  </owl:Class>
  <owl:Class rdf:about="http://CRS.owl#document">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://CMT.owl#Document"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CRS.owl#event"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="http://CRS.owl#person"/>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CRS.owl#program"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:about="http://CMT.owl#Conference">
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Decision"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:minCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:minCardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://CMT.owl#date"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Person"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Document"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://CMT.owl#reviewsPerPaper"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="http://CMT.owl#name"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Bid"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#Preference"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="http://CMT.owl#ProgramCommittee"/>
    </owl:disjointWith>
  </owl:Class>
